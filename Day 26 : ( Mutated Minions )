#include <bits/stdc++.h>
using namespace std;

int main() {
       int T;
       cin>>T;
       while(T--){
           int N, K;
           cin>>N>>K;
           int S[N];
           for(int i=0;i<N;i++){
               cin>>S[i];
           }
           int c=0;
           for(int i=0;i<N;i++){
               if((S[i]+K)%7==0) 
               c++;
           }
           cout<<c<<endl;
       }

}
