#include <bits/stdc++.h>
using namespace std;

int main() {
	int T;
	cin>>T;
	while (T--){
	    int N;
	    cin>>N;
	    int e=0;
	    int o=0;
	    for(int a=1;a<=N;a++){
	        if(N%a==0&&(a%2==0))
	           e++;
	        else if(N%a==0&&(a%2!=0))
	           o++;
	    }
	    if(e>o)
	    cout<<1<<endl;
	    else if(e==o)
	    cout<<0<<endl;
	    else
	    cout<<-1<<endl;
	}

}
