#include <bits/stdc++.h>
#include <string.h>
using namespace std;

int main() {
	int T;
	cin>>T;
	while(T--){
	    int N;
	    cin>>N;
	    string A[N];
	    for(int i=0;i<N;i++){
	        cin>>A[i];
	    }
	    int s=0;
	    int l=0;
	    for (int i=0;i<N;i++){
	        if(A[i]=="START38")
	        s++;
	        else if(A[i]=="LTIME108")
	        l++;
	    }
	    cout<<s<<" "<<l<<endl;
	}

}
